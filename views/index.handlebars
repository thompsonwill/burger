<h1>burgers</h1>
<div class="burger-container">
  <ul>
    {{#each burgers}}
    <p>{{ this.burger_name }}</p>
    {{/each}}
  </ul>
</div>

<div class="eaten">

</div>


<form class="create-form">
  <div class="form-group">
    <label for="burger">Burger Name:</label>
    <input type="text" id="burger_name" name="name">
  </div>
  <button type="submit">Add Burger</button>
</form>